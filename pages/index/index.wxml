<!--index.wxml-->
<view class="page-body">
  <view class="page-section page-section-gap">
    <map
      bindtap="onclickmap"
      bindmarkertap="onclickmarker"
      id="myMap"
      style="width: 100%; height: 1400rpx;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      wx:if="{{hasMarkers}}"
      show-location
    >
    <cover-view class="order-cover-view">
    <!-- <van-icon name="like" class="like-icon"/> -->
    <van-button round icon="like" type="info" bindtap="onclickorder">预约</van-button>
    </cover-view>
    </map>
  </view>
  </view>


